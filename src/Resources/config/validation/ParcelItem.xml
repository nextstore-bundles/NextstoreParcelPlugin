<?xml version="1.0" encoding="UTF-8"?>

<constraint-mapping xmlns="http://symfony.com/schema/dic/constraint-mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/constraint-mapping http://symfony.com/schema/dic/services/constraint-mapping-1.0.xsd">

    <class name="Nextstore\SyliusParcelPlugin\Model\ParcelItem">
        <property name="trackingCode">
            <constraint name="NotBlank">
                <option name="message">nextstore_sylius_parcel.parcel_item.tracking_code.not_blank</option>
                <option
                    name="groups">nextstore_sylius_parcel</option>
            </constraint>
        </property>
    </class>

</constraint-mapping>
